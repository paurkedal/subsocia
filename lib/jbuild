(jbuild_version 1)

(library
 ((name subsocia)
  (public_name subsocia)
  (wrapped false)
  (modules_without_implementation
   (Subsocia_intf
    Subsocia_derived_intf
    Subsocia_schema_types
    Subsocia_selector_types))
  (preprocess (pps (lwt.ppx)))
  (libraries
   (extunix
    lwt
    lwt.log
    lwt.ppx
    panograph
    yojson))))

(ocamlyacc (subsocia_parser))
(ocamllex (subsocia_lexer))
